{
    "model":{
      "id":"treelikelihood",
      "type":"treelikelihood",
      "sse":true,
      "tipstates":false,
      "sitepattern":{
        "id":"patterns",
        "type":"sitepattern",
        "datatype":"nucleotide",
        "alignment":{
          "id":"seqs",
          "type":"alignment",
          "file":"fluA.fa"
        }
      },
      "sitemodel":{
        "id":"sitemodel",
        "type":"sitemodel",
        "distribution":{
          "distribution": "gamma",
          "catgeories": 4,
          "parameters":{
            "id": "alpha",
            "type": "parameter",
            "value": 0.1,
            "lower":0
          }
        }
      },
        "substitutionmodel":{
          "id":"sm",
          "type":"substitutionmodel",
          "model":"gtr",
          "datatype":"nucleotide",
          "frequencies":{
            "id":"freqs",
            "type":"Simplex",
            "values":[
              0.25,
              0.25,
              0.25,
              0.25
            ]
          },
          "rates":{
            "id":"rates",
            "type":"Simplex",
            "dimension": 6
          },
          "init":{
            "sitepattern":"&patterns"
          }
        },
      "tree":{
        "id":"tree",
        "type":"tree",
        "parameters":"tree.distances",
        "file":"fluA-rooted.nxs"
      }
    },
    "physher":[
      {
        "id":"mlopt",
        "type":"optimizer",
        "algorithm":"meta",
        "precision":0.00001,
        "max":10000,
        "min":1,
        "model":"&treelikelihood",
        "list":[
          {
            "id":"optbl",
            "type":"optimizer",
            "algorithm":"serial",
            "model":"&treelikelihood",
            "treelikelihood":"&treelikelihood"
          },
          {
            "id":"optrates",
            "type":"optimizer",
            "algorithm":"serial",
            "model":"&treelikelihood",
            "parameters":"$rates"
          },
          {
            "id":"optfreqs",
            "type":"optimizer",
            "algorithm":"serial",
            "model":"&treelikelihood",
            "parameters":"$freqs"
          },
          {
            "id":"optsite",
            "type":"optimizer",
            "algorithm":"serial",
            "model":"&treelikelihood",
            "parameters":"&alpha"
          }
        ]
      },
      {
        "id":"log",
        "type":"logger",
        "models":"&treelikelihood"
      },
      {
        "id":"log",
        "type":"logger",
        "models":[
          "$rates",
          "$freqs"
        ]
      },
      {
        "id":"log",
        "type":"logger",
        "parameters":[
          "&alpha"
        ]
      },
      {
        "id":"log",
        "type":"logger",
        "models":"&tree",
        "tree":true
      }
    ]
  }